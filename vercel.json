{
  "version": 2,
  "builds": [
    {
      "src": "deepseek32-landing.html",
      "use": "@vercel/static"
    },
    {
      "src": "remixicon-custom.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/remixicon-custom.css",
      "dest": "/remixicon-custom.css",
      "headers": {
        "Content-Type": "text/css; charset=utf-8",
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/sitemap.xml",
      "dest": "/public/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml; charset=utf-8"
      }
    },
    {
      "src": "/robots.txt", 
      "dest": "/public/robots.txt",
      "headers": {
        "Content-Type": "text/plain; charset=utf-8"
      }
    },
    {
      "src": "/deepseek32-landing.html",
      "dest": "/deepseek32-landing.html"
    },
    {
      "src": "/(.*)",
      "dest": "/deepseek32-landing.html"
    }
  ]
}